{
  "name": "react-vite-sdk-widget",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "predev": "tailwindcss -i ./src/styles/widget.css -o ./dist/sdk-widget.css",
    "dev": "concurrently \"vite --host\" \"npm run dev:css\"",
    "build": "concurrently \"tsc --jsx react-jsx && vite build\"",
    "preview": "vite preview",
    "dev:css": "tailwindcss -i ./src/styles/widget.css -o ./dist/sdk-widget.css --watch"
  },
  "dependencies": {
    "lucide-react": "^0.460.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/rollup": "^0.54.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.1.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "packageManager": "pnpm@8.13.1+sha1.90f9b2bb3ed58632bcb7b13c3902d6873ee9501c"
}
